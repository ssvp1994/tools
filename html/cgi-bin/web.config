<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="python" path="*.py" verb="*" modules="CgiModule" scriptProcessor="C:\Users\Administrator\AppData\Local\Programs\Python\Python36-32\python.exe %s %s" resourceType="File" />
        </handlers>
    </system.webServer>
</configuration>
